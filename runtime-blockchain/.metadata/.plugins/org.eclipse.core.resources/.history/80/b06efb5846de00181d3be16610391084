pragma solidity ^0.4.0;

[%for (smc in SmartContract.all) { %]
contract [%=smc.name%] {
 	[%for (ent in smc.entidades) { %]
		struct [%=ent.name%] {
		  [%for (attr in ent.atributos) { %]
			[%=attr.tipodato.name%] [%=attr.name%] 
		[%}%]
		}
		[%for (est in smc.estado) { %]
			mapping ([%=est.tipodato.name%]=>[%=ent.name%]) public [%=est.name%]
		[%}%]
[%}%] 
}
	
[%}%]

 
